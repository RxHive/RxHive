---
layout: null
---

[
{% assign pages = site.composition %}
{% for page in pages %}
  {
    "title": "{{ page.title | escape }}",
    "url": "{{ page.url | relative_url }}",
    "content": {{ page.content | strip_html | normalize_whitespace | truncate: 100 | jsonify }}
  }{% unless forloop.last %},{% endunless %}
{% endfor %}
]
